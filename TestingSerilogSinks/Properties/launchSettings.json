        "OTEL_SERVICE_NAME": "testing-serilog-sinks",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "http://127.0.0.1:4317/",
        "OTEL_RESOURCE_ATTRIBUTES": "deployment.environment=Local"
      }
    }
  }
}

